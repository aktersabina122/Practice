package com.hcl.thrdemo;

import java.util.ArrayList;
import java.util.List;

class Thr1 implements Runnable {

	@Override
	public void run() {
		List<String> names=new ArrayList<String>();
		names.add("Sowmya");
		names.add("Ram");
		names.add("Poornesh");
		names.add("Bharath");
		names.add("Hemanth");
		for (String s : names) {
			System.out.println(s);
			Thread.sleep(1000);
		}
	}
	
}

public class ThrEx {

}
