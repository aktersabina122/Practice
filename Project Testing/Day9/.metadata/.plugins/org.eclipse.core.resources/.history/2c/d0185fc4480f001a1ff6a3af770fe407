package com.hcl.thrdemo;

class MTable {
	void show(int n) {
		int p;
		for(int i=1;i<=10;i++) {
			p=n*i;
			System.out.println(n + " * " +i+ " = " +p);
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}

class Tab1 extends Thread {
	MTable mtable;
	Tab1(MTable mtable) {
		this.mtable=mtable;
	}
	@Override
	public void run() {
		mtable.show(12);
	}
}
public class ThrTable {

}
